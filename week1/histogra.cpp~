#include <iostream>
#include <math.h>
using namespace std;

int main(){
    int t;
    cin>>t;
    while(t){
        int n; cin>>n; // n = number of rectangles
        int h[n];      // array containing height of rectangles
        int w=1;       // width of each rectangle
        for(int i=0; i<n; i++){
            cin>>h[i];
        }
        int max=h[0]*w;
    }
    return 0;
}